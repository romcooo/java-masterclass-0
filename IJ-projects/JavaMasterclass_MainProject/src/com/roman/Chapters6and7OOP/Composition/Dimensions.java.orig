package com.roman.Chapters6and7OOP.Composition;

public class Dimensions {
    private int width;
    private int height;
    private int depth;

    public Dimensions(final int width, final int height, final int depth) {
        this.width = width;
        this.height = height;
        this.depth = depth;
    }

    public int getWidth() {
        return width;
    }

    public int getHeight() {
        return height;
    }

    public int getDepth() {
        return depth;
    }
}
